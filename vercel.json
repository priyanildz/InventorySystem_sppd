{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|jpeg))",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ]
}